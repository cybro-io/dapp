    "axios": "^1.7.2",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.5",
    "ethers": "^6.12.1",
    "framer-motion": "^11.1.9",
    "mixpanel-browser": "^2.52.0",
    "next": "14.2.3",
    "numeral": "^2.0.6",
    "postcss": "^8.4.38",
    "react": "^18",
    "react-dom": "^18",
    "react-google-recaptcha": "^3.1.0",
    "react-hook-form": "^7.52.1",
