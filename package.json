{
  "name": "d-app",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "prepare": "husky",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "linter": "eslint '{**/*,*}.{js,ts,jsx,tsx}'",
    "linter:fix": "eslint --fix '{**/*,*}.{js,ts,jsx,tsx}'",
    "stylelint": "stylelint '**/*.scss'",
    "stylelint:fix": "stylelint '**/*.scss' --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "lint:staged": "lint-staged",
    "lint:all": "npm run prettier:fix && npm run linter:fix && npm run stylelint:fix",
    "test": "npm run prettier && npm run linter && npm run stylelint",
    "codegen-sc": "typechain --target ethers-v5 --out-dir src/shared/types/__generated/contracts './src/app/abi/*.json'",
    "codegen-api": "npx orval",
    "generate-index": "node src/shared/script/generate-index.mjs",
    "codegen-sc-with-index": "npm run codegen-sc && npm run generate-index",
    "codegen-api-with-index": "npm run codegen-api && npm run generate-index",
    "codegen": "npm run codegen-sc && npm run codegen-api && npm run generate-index"
  },
  "lint-staged": {
    "*.!({js,ts,jsx,tsx})": "prettier --write --ignore-unknown",
    "*.{js,ts,jsx,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.scss": "stylelint --fix"
  },
  "dependencies": {
    "@types/mixpanel-browser": "^2.49.0",
    "@types/numeral": "^2.0.5",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-google-recaptcha": "^2.1.9",
    "@types/react-window": "^1.8.8",
    "@types/recharts": "^1.8.29",
    "@ebay/nice-modal-react": "^1.2.13",
    "@hookform/resolvers": "^3.9.0",
    "@next/third-parties": "^14.2.5",
    "@nextui-org/react": "^2.3.6",
    "@sentry/nextjs": "^8.30.0",
    "@svgr/webpack": "^8.1.0",
    "@tanstack/react-query": "^5.45.1",
    "@web3modal/ethers5": "^5.0.11",
    "autoprefixer": "^10.4.19",
    "axios": "^1.7.2",
    "bignumber.js": "^9.1.2",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "effector": "^23.2.2",
    "effector-react": "^23.2.1",
    "ethereum-multicall": "^2.26.0",
    "ethers": "^5.7.2",
    "formik": "^2.4.6",
    "framer-motion": "^11.1.9",
    "mixpanel-browser": "^2.52.0",
    "next": "14.2.3",
    "nextjs-toploader": "^1.6.12",
    "numeral": "^2.0.6",
    "orval": "^6.30.2",
    "postcss": "^8.4.38",
    "react": "^18",
    "react-dom": "^18",
    "react-google-recaptcha": "^3.1.0",
    "react-hook-form": "^7.52.1",
    "react-toastify": "^10.0.5",
    "react-virtualized-auto-sizer": "^1.0.24",
    "react-window": "^1.8.10",
    "recharts": "^2.12.7",
    "sharp": "^0.33.5",
    "symbiosis-js-sdk": "^3.1.140",
    "tailwindcss": "^3.4.3",
    "tailwindcss-animated": "^1.1.2",
    "usehooks-ts": "^3.1.0",
    "victory": "^37.0.2",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@typechain/ethers-v5": "^11.1.2",
    "@tanstack/eslint-plugin-query": "^5.43.1",
    "@types/node": "^20",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "sass": "^1.77.0",
    "stylelint": "^16.5.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-order": "^6.0.4",
    "stylelint-prettier": "^5.0.2",
    "stylelint-scss": "^6.3.0",
    "ts-node": "^10.9.2",
    "typechain": "^8.3.2",
    "typescript": "^5",
    "typescript-eslint": "^7.8.0"
  }
}
